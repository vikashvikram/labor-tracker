<p id="notice"><%= notice %></p>

<h1>Listing Medical Tests</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Place</th>
      <th>User</th>
      <th>Patient</th>
      <th>Weight</th>
      <th>Tobacco</th>
      <th>Alcohol</th>
      <th>Heart rate</th>
      <th>Fetal heart sound</th>
      <th>Uterus contraction</th>
      <th>Temperature</th>
      <th>Sugar</th>
      <th>Blood pressure</th>
      <th>Folic acid supplimentation level</th>
      <th>State</th>
      <th>Hemoglobin</th>
      <th>Platelet count</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @medical_tests.each do |medical_test| %>
      <tr>
        <td><%= medical_test.date %></td>
        <td><%= medical_test.place %></td>
        <td><%= medical_test.user_id %></td>
        <td><%= medical_test.patient_id %></td>
        <td><%= medical_test.weight %></td>
        <td><%= medical_test.tobacco %></td>
        <td><%= medical_test.alcohol %></td>
        <td><%= medical_test.heart_rate %></td>
        <td><%= medical_test.fetal_heart_sound %></td>
        <td><%= medical_test.uterus_contraction %></td>
        <td><%= medical_test.temperature %></td>
        <td><%= medical_test.sugar %></td>
        <td><%= medical_test.blood_pressure %></td>
        <td><%= medical_test.folic_acid_supplimentation_level %></td>
        <td><%= medical_test.state %></td>
        <td><%= medical_test.hemoglobin %></td>
        <td><%= medical_test.platelet_count %></td>
        <td><%= link_to 'Show', medical_test %></td>
        <td><%= link_to 'Edit', edit_medical_test_path(medical_test) %></td>
        <td><%= link_to 'Destroy', medical_test, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Medical test', new_medical_test_path %>
